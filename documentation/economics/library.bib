@article{Munch2017,
abstract = {This study compares the precision of the existing approach 'speed rule' to a probability model categorizing VMS data as fishing locations. Using data from Northeast U.S. Fisheries for the fishing years 2010-2014, we show that the traditional representation of fishing activities as derived by speed rules leads to a severe misrepresentation of fishing grounds for gears other than bottom otter trawl. Predictions based on probability models outperform the simple speed rule in classifying VMS polls for sink gillnet and scallop dredge trips, gears with more complicated patterns of fishing than bottom otter trawlers, without adding substantial computational effort. While the speed rule tends to perform well for the classification of bottom otter trawl fishing locations, the probability model described performs at least as well in terms of prediction, and provides added value in terms of a conditional probability of fishing for each VMS poll, which can be used to weight these polls in research extensions. We also highlight issues, such as fleet dynamics, that historically have not been dealt with in the static speed rules most often used to predict fishing locations, but which can have significant impacts on the quality of predictions, and are most easily controlled for in a formal probability model. JEL: Q22},
author = {M{\"{u}}nch, Angela and Depiper, Geret Sean and Demarest, Chad},
keywords = {VMS,fishing location,precision},
title = {{On the precision of predicting fishing location using data from the Vessel Monitoring System (VMS)}},
year = {2017}
}
@article{Jenkins2005,
abstract = {This paper develops methods for decomposing changes in the income distribution using subgroup decompositions of the income density function. Overall changes are related to changes in subgroup shares and changes in subgroup densities, where the latter are broken down further using elementary transformations of individual incomes. These density decompositions are analogous to the widely-used decompositions of inequality indices by population subgroup, except that they summarize multiple features of the income distribution (using graphs), rather than focusing on a specific feature such as dispersion, and are not dependent on the choice of a specific summary index. Nonetheless, since inequality and poverty indices can be expressed as PDF functionals, our density-based methods can also be used to provide numerical decompositions of these. An application of the methods reveals the multi-faceted nature of UK income distribution trends during the 1980s.},
author = {Jenkins, Stephen P and {Van Kerm}, Philippe},
doi = {10.1007/s10888-004-8309-1},
file = {:C$\backslash$:/Users/Min-Yang.Lee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jenkins, Van Kerm - 2005 - Accounting for income distribution trends A density function decomposition approach.pdf:pdf},
