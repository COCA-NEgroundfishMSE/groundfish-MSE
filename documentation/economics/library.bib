@article{Smith2005a,
abstract = {Commercial fishing involves both physical and financial risks. This$\backslash$ncombination questions whether fishermen are inherently risk-loving,$\backslash$nwhether physical and financial risk preferences are correlated, and$\backslash$nhow much preferences vary across fishermen. This paper addresses$\backslash$nthese questions with a panel data set of daily participation decisions$\backslash$nin the California sea urchin dive fishery. Weather buoy data and$\backslash$nthe prevalence of great white sharks at a particular fishing site$\backslash$nproxy for physical risk. Overall, urchin fishermen are not risk-loving$\backslash$non average, risk preferences are heterogeneous, and there is some$\backslash$nevidence that risk preferences are positively correlated across physical$\backslash$nand financial domains.},
