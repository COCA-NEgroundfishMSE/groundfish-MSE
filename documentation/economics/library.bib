@article{Morrison1999,
abstract = {Scale economies and agglomeration externalities are alleged to be important determinants of economic growth. To assess these effects, we outline and estimate a microfoundations model based on a dynamic cost function specification. This model provides for the separate identification of the impacts of extemalities and cyclical utilization on short-and long-run scale economies and input substitution pattems. We find that scale economies are prevalent in U.S. manufacturing, cost savings and scale effects often attributed to intemal inputs may be due to extemal factors, and supply-side agglomeration effects are greater than demand-side, especially in the long run. (JEL O47, E23) The importance of agglomeration extemal-ities for explaining cyclical fluctuations and growth has been examined in various fields in economics, including the literature on techni-cal change and productivity growth; business cycles; regional economic growth and location theory; and endogenous economic growth. Agglomeration or thick market effects are of-ten assumed to operate through knowledge spillovers that may result from activity levels of suppliers, customers, and other firms in an industry. These extemalities appear as scale economies, which must be identified sepa-rately from fluctuations arising from intemal factors such as short-run fixities (cyclical uti-lization) and long-run retums to scale, in order to evaluate the determinants of growth. Robert E. Hall (1990) identified scale ef-fects arising from thick markets as a potential cause for invariance of the Solow residual. Following this lead, Ricardo J. Caballero and Richard K. Lyons (henceforth, CL) (1992) and Eric J. Baitlesman et al. (henceforth, BCL) (1994) pursued the issue of agglomeration ef-fects in more detail, distinguishing between short-and long-run extemalities. Agglomeration},
author = {Morrison, Catherine J.M.P. and Siegel, Donald S},
file = {:C$\backslash$:/Users/Min-Yang.Lee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Morrison, Siegel - 1999 - Scale Economies and Industry Agglomeration Externalities A Dynamic Cost Function Approach.pdf:pdf},
journal = {American Economic Review},
number = {1},
pages = {272--290},
title = {{Scale Economies and Industry Agglomeration Externalities: A Dynamic Cost Function Approach}},
volume = {89},
year = {1999}
}
