journal = {Marine Resource Economics},
keywords = {fisheries management,i18,individual fi shing quotas,j28,jel codes,q22,q28,risk,safety},
number = {1},
pages = {000--000},
title = {{A Safer Catch? The Role of Fisheries Management in Fishing Safety}},
volume = {37},
year = {2022}
}
