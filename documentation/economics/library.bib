journal = {Marine Policy},
keywords = {Common property,Cost recovery,Fisheries economics,Fisheries management},
number = {3},
pages = {233--243},
title = {{Costs of fisheries management: The cases of Iceland, Norway and Newfoundland}},
volume = {24},
year = {2000}
}
