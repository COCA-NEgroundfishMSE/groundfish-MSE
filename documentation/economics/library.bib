@article{Gloaguen2015,
abstract = {The understanding of the dynamics of fishing vessels is of great{\$}\backslash{\$}ninterest to characterize the spatial distribution of the fishing effort{\$}\backslash{\$}nand to define sustainable fishing strategies. It is also a prerequisite{\$}\backslash{\$}nfor anticipating changes in fishermen's activity in reaction to{\$}\backslash{\$}nmanagement rules, economic context, or evolution of exploited resources.{\$}\backslash{\$}nAnalyzing the trajectories of individual vessels offers promising{\$}\backslash{\$}nperspectives to describe the activity during fishing trips. A hidden{\$}\backslash{\$}nMarkov model with two behavioral states (steaming and fishing) is{\$}\backslash{\$}ndeveloped to infer the sequence of non-observed fishing vessel behavior{\$}\backslash{\$}nalong the vessel trajectory based on Global Positioning System (GPS){\$}\backslash{\$}nrecords. Conditionally to the behavior, vessel velocity is modeled with{\$}\backslash{\$}nan autoregressive process. The model parameters and the sequence of{\$}\backslash{\$}nhidden behavioral states are estimated using an expectation-maximization{\$}\backslash{\$}nalgorithm, coupled with the Viterbi algorithm that captures the most{\$}\backslash{\$}ncredible joint sequence of hidden states. A simulation approach was{\$}\backslash{\$}nperformed to assess the influence of contrast between the model{\$}\backslash{\$}nparameters and of the path length on the estimation performances. The{\$}\backslash{\$}nmodel was then fitted to four original GPS tracks recorded with a time{\$}\backslash{\$}nstep of 15min derived from volunteer fishing vessels operating in the{\$}\backslash{\$}nChannel within the IFREMER RECOPESCA project. Results showed that the{\$}\backslash{\$}nfishing activity performed influenced the estimates of the velocity{\$}\backslash{\$}nprocess parameters. Results also suggested future inclusion of variables{\$}\backslash{\$}nsuch as tide currents within the ecosystem approach of fisheries.{\$}\backslash{\$}nCopyright (c) 2014 John Wiley {\&} Sons, Ltd.},
author = {Gloaguen, P and Mah{\'{e}}vas, S and Rivot, E and Woillez, M and Guitton, J and Vermard, Y and Etienne, M P},
doi = {10.1002/env.2319},
isbn = {1180-4009},
issn = {1099095X},
journal = {Environmetrics},
keywords = {Autoregressive process,Baum-Welch algorithm,Hidden Markov model,RECOPESCA,Vessels dynamics},
number = {1},
pages = {17--28},
title = {{An autoregressive model to describe fishing vessel movement and activity}},
