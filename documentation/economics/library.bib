doi = {10.1016/j.jeem.2007.11.003},
file = {:C$\backslash$:/Users/Min-Yang.Lee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Smith, Zhang, Coleman - 2008 - Econometric modeling of fisheries with complex life histories Avoiding biological management failures.pdf:pdf},
