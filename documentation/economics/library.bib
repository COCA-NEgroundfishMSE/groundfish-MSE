file = {:C$\backslash$:/Users/Min-Yang.Lee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pesaran, Shin, Smith - 2001 - Bounds Testing Approaches to the Analysis of Level Relationships.pdf:pdf},
isbn = {08837252},
journal = {Journal of Applied Econometrics},
number = {3},
pages = {289--326},
primaryClass = {10.17863},
title = {{Bounds Testing Approaches to the Analysis of Level Relationships}},
volume = {16},
year = {2001}
}
